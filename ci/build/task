#!/bin/sh

set -eux

my_dir="$( cd "$( dirname "${0}" )" && pwd )"
base_dir="$( cd "${my_dir}/../.." && pwd )"
scripts_dir="$( cd "${base_dir}/scripts" && pwd )"

export GOPATH=$PWD/gopath
export PATH=$GOPATH/bin:$PATH

cp "${PWD}/s3-out-binary/s3-out" "${base_dir}/s3-out"

"${scripts_dir}/build" "$@"
